
entity SignatureLevel {
	orgName String,
    orgId String,
    level1 Integer,
    level2 Integer,
    level3 Integer,
    completed Boolean
}

enum StatusEnum {
    NONE, ON_GOING, DONE, FAILED
}

entity Status {
	status StatusEnum,
    exception Blob,
    month Integer,
    year Integer,
    totalToCreate Integer,
    totalCreated Integer
}

relationship OneToMany {
	Status{signatureLevels} to SignatureLevel 
}